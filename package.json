{
  "name": "angular-rollbar",
  "version": "0.1.3",
  "description": "Angular 2+ Service for Rollbar error logging",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "clean": "rm -rf lib",
    "doc": "./node_modules/.bin/typedoc --out doc --mode modules --excludeNotExported --excludeExternals src",
    "test": "ng test --single-run --code-coverage",
    "posttest": "npm run coveralls",
    "tsc": "node_modules/.bin/tsc",
    "ngc": "node_modules/.bin/ngc",
    "build": "npm run ngc",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "coveralls": "./node_modules/coveralls/bin/coveralls.js < coverage/coverage.lcov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextFaze/angular-rollbar"
  },
  "keywords": [
    "angular",
    "rollbar",
    "logging"
  ],
  "author": "Michael Marner <npm-support@nextfaze.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/cli": "^1.6.7",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codecov": "^1.0.1",
    "codelyzer": "^4.0.1",
    "coveralls": "^2.11.16",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "mocha": "^3.2.0",
    "protractor": "~5.1.2",
    "rxjs": "^5.0.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "tslint-eslint-rules": "^3.3.0",
    "typedoc": "^0.5.9",
    "typescript": "~2.4.2",
    "webpack": "^2.1.0-beta.25",
    "zone.js": "^0.8.4"
  },
  "peerDependencies": {
    "@angular/common": ">=2.0.0",
    "@angular/core": ">=2.0.0"
  },
  "dependencies": {
    "rollbar": "^2.3.8"
  }
}
